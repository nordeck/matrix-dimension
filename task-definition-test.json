{
    "family": "phoenix-dimension-task-test",
    "networkMode": "host",
    "containerDefinitions": [
        {
            "name": "phoenix-dimension, 
            "image": "751489943657.dkr.ecr.eu-central-1.amazonaws.com/phoenix-dimension:test-latest",
            "logConfiguration": {
                "logDriver": "awslogs",
                "secretOptions": null,
                "options": {
                    "awslogs-group": "/ecs/phoenix-dimension-task-test",
                    "awslogs-region": "eu-central-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "portMappings": [
                {
                    "containerPort": 8400, 
                    "hostPort": 8480, 
                    "protocol": "tcp"
                },
                {
                    "containerPort": 80,
                    "hostPort": 8280,
                    "protocol": "tcp"
                }
            ],
            "environment": [
                {
                    "name": "NODE_ENV",
                    "value": "test"
                }
            ],
            "essential": true,
            "hostname": "phoenix-dimension-test"
        }
    ], 
    "requiresCompatibilities": ["EC2"],
    "cpu": "128",
    "memory": "256"
}